    <!-- Add Session Form -->
    <div class="add" id="add">
      <div class="col-md-6 offset-md-3">
        <div class="card mb-3">
          <div class="card-body">
            <div class="pt-4 pb-2">
              <h5 class="card-title text-center pb-0 fs-4">Add Session</h5>
              <!-- <p class="text-center small">Add Session</p> -->
            </div>
            <form class="row g-3 needs-validation" novalidate>
              <div class="col-12">
                <label for="yourName" class="form-label">Session Name</label>
                <input type="text" name="name" class="form-control" id="yourName" required>
                <div class="invalid-feedback">Please, enter the session name!</div>
              </div>

              <div class="col-12">
                <label for="yourEmail" class="form-label">Starting Date</label>
                <input type="date" name="email" class="form-control" id="date" required>
                <div class="invalid-feedback">Please enter Starting Date!</div>
              </div>

              <div class="col-12">
                <label for="yourEmail" class="form-label">Finishing Date</label>
                <input type="date" name="email" class="form-control" id="date" required>
                <div class="invalid-feedback">Please enter Finishing Date!</div>
              </div>

              <div class="col-12">
                <button class="btn btn-primary w-100" type="submit" onclick="closeForm()">Add Session</button>
              </div>
            </form>
            <label class="form-check-label" for="acceptTerms"><a href="#" onclick="closeForm()">Close
                form</a></label>
          </div>
        </div>
