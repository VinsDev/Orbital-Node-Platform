            <div class="card-body">
              <h5 class="card-title">Students</h5>
              <b>To delete a student, double click on the Student's row.</b>
              <div style="height: 20px;"></div>
              <% if(sessions_data[0].terms[0].students.length<1){ %>
                <div class="text-center">
                  <h6>
                    No student added to selected class yet. You can check other classes or add students to this
                    one by pressing the button below.
                  </h6>
                </div>
                <% } else { %>
                  <!-- Table with hoverable rows -->
                  <table class="table table-hover" id="myTable">
                    <thead>
                      <tr>
                        <th scope="col">S/N</th>
                        <th scope="col">Name</th>
                        <th scope="col">Gender</th>
                        <th scope="col">DOB</th>
                        <th scope="col">Class</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%counter=1;%>

                        <% for(var j=0; j < sessions_data[0].terms[0].students.length; j++){ %>
                          <% if(sessions_data[0].terms[0].students[j].class===class_data[0].name) { %>
                            <tr data-st-id="<%=sessions_data[0].terms[0].students[j].name%>" class="stdRow">
                              <th scope="row">
                                <%=counter %>
                              </th>
                              <td>
                                <%= sessions_data[0].terms[0].students[j].name %>
                              </td>
                              <td>
                                <%= sessions_data[0].terms[0].students[j].gender %>
                              </td>
                              <td>
                                <%= sessions_data[0].terms[0].students[j].dob %>
                              </td>
                              <td>
                                <%= sessions_data[0].terms[0].students[j].class %>
                              </td>
                            </tr>

                    </tbody>
                  </table>
                  <!-- End Table with hoverable rows -->
                  <%counter++;%>
                    <% } %>
                      <% } %>
                        <% } %>
            </div>
          </div>
          <div class="text-center">
            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#addStudentForm"
              id="adstd">Add
              Student</button>
          </div>
        </div>
        </div>
